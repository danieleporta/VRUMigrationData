<process name="VRUMIgrationCheck"
               targetNamespace="http://xmlns.oracle.com/VRUMigration/VRUMIgrationCheck/VRUMIgrationCheck"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/VRUMigration/VRUMIgrationCheck/VRUMIgrationCheck"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:ui="http://xmlns.oracle.com/soa/designer"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable" xmlns:ns1="http://www.webserviceX.NET"
         xmlns:ns2="http://oagj.bpel/VRUMigration/VRUMigrationCheck"
         xmlns:ns3="http://xmlns.oracle.com/VRUMigration/VRUMIgrationCheck/VRUMIgrationCheck/correlationset"
         xmlns:ns4="http://www.webservicex.net/" xmlns:ns5="http://oagj.webservices"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns6="http://xmlns.oracle.com/VRUMigration/VRUMIgrationCheck/MediatorPreCheck">
  <bpelx:annotation>
    <bpelx:analysis>
      <bpelx:property name="propertiesFile">
        <![CDATA[../WSDLs/VRUMIgrationCheck_properties.wsdl]]>
      </bpelx:property>
    </bpelx:analysis>
  </bpelx:annotation>
  <import ui:processWSDL="true" namespace="http://xmlns.oracle.com/VRUMigration/VRUMIgrationCheck/VRUMIgrationCheck"
          location="../WSDLs/VRUMIgrationCheck.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="vrumigrationcheck_client" partnerLinkType="client:VRUMIgrationCheck" myRole="VRUMIgrationCheckProvider"/>
    <partnerLink name="MediatorPreCheck.MediatorPreCheck" partnerLinkType="ns6:MediatorPreCheck.MediatorPreCheck"
                 partnerRole="execute_ptt"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:VRUMIgrationCheckRequestMessage"/>

    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable" messageType="client:VRUMIgrationCheckResponseMessage"/>
    <variable name="InvokeMediator_execute_InputVariable" messageType="ns6:requestMessage"/>
    <variable name="InvokeMediator_execute_OutputVariable" messageType="ns6:replyMessage"/>
  </variables>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in VRUMIgrationCheck.wsdl) -->
    <receive name="receiveInput" partnerLink="vrumigrationcheck_client" portType="client:VRUMIgrationCheck" operation="process" variable="inputVariable" createInstance="yes"/>
    <assign name="AssignRequest">
      <copy>
        <from>$inputVariable.payload/ns2:requestId</from>
        <to>$InvokeMediator_execute_InputVariable.request/ns5:VRUPreCheckMigrationDataRequest/ns5:requestId</to>
      </copy>
      <copy>
        <from>$inputVariable.payload/ns2:ctx/ns2:clientId</from>
        <to>$InvokeMediator_execute_InputVariable.request/ns5:VRUPreCheckMigrationDataRequest/ns5:ctx/ns5:clientId</to>
      </copy>
      <copy>
        <from>$inputVariable.payload/ns2:ctx/ns2:username</from>
        <to>$InvokeMediator_execute_InputVariable.request/ns5:VRUPreCheckMigrationDataRequest/ns5:ctx/ns5:username</to>
      </copy>
      <copy ignoreMissingFromData="yes">
        <from>$inputVariable.payload/ns2:ctx/ns2:password</from>
        <to>$InvokeMediator_execute_InputVariable.request/ns5:VRUPreCheckMigrationDataRequest/ns5:ctx/ns5:password</to>
      </copy>
      <copy ignoreMissingFromData="yes">
        <from>$inputVariable.payload/ns2:ctx/ns2:transactionId</from>
        <to>$InvokeMediator_execute_InputVariable.request/ns5:VRUPreCheckMigrationDataRequest/ns5:ctx/ns5:transactionId</to>
      </copy>
    </assign>
    <invoke name="InvokeMediator" partnerLink="MediatorPreCheck.MediatorPreCheck"
            portType="ns6:execute_ptt" operation="execute" inputVariable="InvokeMediator_execute_InputVariable"
            outputVariable="InvokeMediator_execute_OutputVariable" bpelx:invokeAsDetail="no"/>
    <assign name="AssignResponse">
      <copy>
        <from>$InvokeMediator_execute_OutputVariable.reply/ns5:VRUPreCheckMigrationDataResp/ns5:responseWithMessage/ns5:resultCode</from>
        <to>$outputVariable.payload/ns2:resultCode</to>
      </copy>
      <copy>
        <from>$InvokeMediator_execute_OutputVariable.reply/ns5:VRUPreCheckMigrationDataResp/ns5:responseWithMessage/ns5:resultDescription</from>
        <to>$outputVariable.payload/ns2:resultDescription</to>
      </copy>
      <copy ignoreMissingFromData="yes">
        <from>$InvokeMediator_execute_OutputVariable.reply/ns5:VRUPreCheckMigrationDataResp/ns5:responseWithMessage/ns5:messageWithParams</from>
        <to>$outputVariable.payload/ns2:messageWithParams</to>
      </copy>
    </assign>
    <!-- Generate reply to synchronous request -->
    <reply name="replyOutput" partnerLink="vrumigrationcheck_client" portType="client:VRUMIgrationCheck" operation="process" variable="outputVariable"/>
  </sequence>
</process>